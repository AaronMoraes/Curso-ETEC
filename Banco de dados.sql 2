create database Contrec;
use Contrec;
create table tudoecia(
boleto varchar(40)not null primary key,
nome integer not null,
ID varchar(50) ,
descricao varchar(40) ,
valor decimal(10,2) ,
vencimento varchar(10) , 
banco varchar(10) );


select * from tudoecia;


